# Environment Variables Template
# Copy this file to .env.local and fill in your values

# Google Calendar booking URL
VITE_GOOGLE_CALENDAR_URL=https://calendar.google.com/calendar/appointments/schedules/YOUR_SCHEDULE_ID

# Personal Information
VITE_YOUR_NAME=Your Full Name
VITE_YOUR_EMAIL=your.email@example.com
VITE_YOUR_LOCATION=San Francisco, CA

# Social Media Links
VITE_GITHUB_URL=https://github.com/yourusername
VITE_LINKEDIN_URL=https://linkedin.com/in/yourprofile
VITE_MEDIUM_URL=https://medium.com/@yourusername
